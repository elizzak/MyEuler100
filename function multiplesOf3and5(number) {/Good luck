function multiplesOf3and5(number) {
  // Good luck!
  return multiplesof3(number) + multiplesof5(number);
}

function multiplesof3(number) {
    var total = 0
    for (var i=1; i<number; i++) {
        var next = i*3
        total += next
    }
    return total    

}

function multiplesof5(number) {
     var total;
    for (var i=1; i<number; i++) {
        var next = i*3
        total += next
    }
    return total 
}


multiplesOf3and5(1000);
